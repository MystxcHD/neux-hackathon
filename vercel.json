{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [

      {
        "source": "/(.*)", 
        "destination": "/index.html"
      }
    ],
    "routes": [

      { "src": "/login", "dest": "/login.html" },
      { "src": "/signup", "dest": "/signup.html" },
      { "src": "/app", "dest": "/app.html" },
  

      { "src": "/", "dest": "/index.html" },
  

      { "src": "/(.*)", "dest": "/index.html" } 
    ]
  }